{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start --transformer node_modules/react-native-typescript-transformer/index.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\""
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "axios": "1.2.2",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-native": "0.70.6",
    "react-native-web": "0.18.10",
    "react-router-dom": "6.6.1",
    "react-scripts": "5.0.1",
    "web-vitals": "2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "18.0.27",
    "@types/react-helmet": "6.1.6",
    "@types/react-native": "0.71.0",
    "prettier": "^2.8.3",
    "typescript": "4.9.4"
  }
}
